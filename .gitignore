# gitignore template for Drupal 8 projects
#
# earlier versions of Drupal are tracked in `community/PHP/`

# Ignore configuration files that may contain sensitive information
/sites/*/local.settings.php

!/web/sites/default/settings.php
!/web/sites/default/services.yml

# Ignore paths that may contain user-generated content
/web/sites/*/files
/web/sites/default
/web/sites/*/public
/web/sites/*/private
/web/sites/*/files-public
/web/sites/*/files-private

# Ignore paths that may contain temporary files
/web/sites/*/translations
/web/sites/*/tmp
/web/sites/*/cache

# Ignore testing related files
/web/sites/simpletest

# Ignore drupal core (if not versioning drupal sources)
/web/core
/web/modules/README.txt
/web/modules/contrib
/web/profiles/README.txt
/web/sites/README.txt
/web/sites/example.sites.php
/web/sites/example.settings.local.php
/web/sites/development.services.yml
/web/themes/README.txt
/web/.csslintrc
/web/.editorconfig
/web/.eslintignore
/web/.eslintrc.json
/web/.gitattributes
/web/.htaccess
/web/.ht.router.php
/web/INSTALL.txt
/web/index.php
/web/LICENSE.txt
/web/README.md
/web/update.php
/web/web.config
/web/robots.txt
/web/autoload.php
/web/example.gitignore
/web/themes/custom/lesnik/.sass-cache
/vendor


/.idea
/data/*
*/.sass-cache

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore environment information
#/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/development.services.yml
/web/sites/development.services.yml
/web/sites/default/private_files/.htaccess
/web/sites/default/private_files/webform/*
/gulpconfig.local.js
/gulpconfig.local.json

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore node modules
node_modules

# Ignore npm debug
npm-debug.log

# Ignore content sync settings
.dev/mysql_dumps/

.DS_Store
node_modules
/dist

# local env files
.env.local
.env.*.local

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

.yarnrc
/.yarn

# Editor directories and files
.idea
.vscode
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw*
/web/themes/custom/compo/assets/*
/web/themes/custom/compo/assets/css/*
/web/themes/custom/compo/assets/js/*
/web/themes/custom/compo/assets/img/*

# lando
.lando.yml

# git scripts
*git.sh
